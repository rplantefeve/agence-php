{% extends "::base.html.twig" %}

{% block title %}RocketfireAgenceMainBundle:CompagnieAerienne:createCompagnieAerienne{% endblock %}

{% block body %}
    <h1>Compagnie aérienne création</h1>
    {{ form_start(form) }}
    {{ form_row(form.nom) }}
    {{ form_widget(form) }}
    <input type="submit" value="Create" class="btn btn-warning btn-lg col-sm-4 navbar-right" />
    {{ form_end(form) }}

    <a href="{{ path('CompagnieAerienne_list') }}">
        <button type="button" class="btn btn-info btn-lg col-sm-4">
            Retour
        </button>
    </a>

{% endblock %}

{% extends "base.html.twig" %}
{% block title %}Inscription d'un particulier{% endblock %}
{% block stylesheets %}<link href="{{ asset('bundles/rocketfireagencemain/style.css') }}" rel="stylesheet">{% endblock %}
{% block body %}
    <body>
        <fieldset>
            <legend><h1>Particulier</h1></legend>
            {{ form_start(form) }}
            <table>
                <tr>
                    <td>Nom </td>
                    <td>{{ form_row(form.client.nom, {'label': ' '}) }}</td>
                </tr>
                <tr>
                    <td>Prénom </td>
                    <td>{{ form_row(form.client.prenom, {'label': ' '}) }}</td>
                </tr>
                <tr>
                    <td>N° Tel </td>
                    <td>{{ form_row(form.client.numTel, {'label': ' '}) }}</td>
                </tr>
                <tr>
                    <td>Email </td>
                    <td>{{ form_row(form.client.eMail, {'label': ' '}) }}</td>
                </tr>

                {% include 'RocketfireAgenceMainBundle:Adresse:new.html.twig' %}
                {% include 'RocketfireAgenceMainBundle:Login:new.html.twig' %}
            </table>
            <br /><button class='button1' type="submit">Valider</button>
            <button class='button1' formaction="{{ path('inscrire_client') }}" formnovalidate>Retour</button>
            {{ form_end(form) }}
        </form>
    </fieldset>
</body>
{% endblock %}
